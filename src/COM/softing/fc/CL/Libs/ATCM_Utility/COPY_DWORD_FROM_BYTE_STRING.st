(* returns a value copied from the byte string BSTR at the position POS *)

FUNCTION COPY_DWORD_FROM_BYTE_STRING : DWORD
VAR_IN_OUT
    POS : DINT;
    BSTR : BYTE_STRING;
END_VAR

    COPY_DWORD_FROM_BYTE_STRING := LWORD_TO_DWORD(
        COPY_NUMERIC_FROM_BYTE_STRING(4,POS,BSTR));  

END_FUNCTION
